{
  "name": "POI",
  "plural": "POIs",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "coordinates": {
      "type": "geopoint",
      "required": true
    },
    "category": {
      "type": "string",
      "default": "unknown category"
    },
    "tourOrder": {
      "type": "number",
      "default": -1
    },
    "description": {
      "type": "string",
      "default": "no description"
    },
    "rating": {
      "type": "decimal",
      "default": 0
    },
    "openingHours": {
      "type": "string"
    },
    "counter": {
      "type": "number",
      "default": 0
    },
    "lastModified": {
      "type": "Date",
      "required": true
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
